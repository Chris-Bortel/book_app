<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include ( '../layout/head' ) %>
 
  </head>
  <body>
    <header>
      <h1> <%= rows %> Saved Books</h1>
    </header>
    <section>

      <% array.forEach( book => { %>

      <div>
      <img src=  "<%= book.image_url %>" alt="">
      <h2> <%= book.title %> </h2>
      <h3> <%= book.author %> </h3>
      <button> View Details </button>
      </div>
      <% }) %>

    </section>
    
  </body>
</html>
